<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
	<metadata>
		<id>ZeroORM.Extensibility</id>
		<version>$version$</version>
		<authors>George Bessonov</authors>
		<owners>bessgeor</owners>
		<licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
		<projectUrl>https://github.com/bessgeor/ZeroORM.Extensibility</projectUrl>
		<iconUrl>http://github.com/bessgeor/ZeroORM/raw/master/icon.png</iconUrl>
